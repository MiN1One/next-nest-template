{
  "name": "nest-next-template",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": false,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild:server": "del-cli \"!./dist/.next\" \"./dist/*\"",
    "build:server": "cross-env NODE_ENV=production nest build",
    "build:client": "cd src/client && cross-env NODE_ENV=production next build",
    "start:build": "del-cli \"./dist\" && cross-env NODE_ENV=build PORT=5000 nest build --path tsconfig.server.json --webpack",
    "build": "node build.script.js",
    "start:server": "nest start",
    "dev": "del-cli \"./dist\" && cross-env NODE_ENV=development nest build --path tsconfig.server.json --webpack --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main"
  },
  "dependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.9",
    "@nestjs/platform-fastify": "^9.3.9",
    "@nestjs/schematics": "^9.0.4",
    "@types/node": "^16.0.0",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fastify": "^4.13.0",
    "i18next": "^22.4.10",
    "next": "^13.1.6",
    "next-i18next": "^13.1.5",
    "prettier": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.1.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "run-script-webpack-plugin": "^0.1.1",
    "rxjs": "^7.2.0",
    "source-map-support": "^0.5.20",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4",
    "webpack": "^5.75.0",
    "webpack-node-externals": "^3.0.0"
  }
}
